<h2>new photo</h2>
<%= form_for @photo do |form| %>


    <div class="">
      <%= form.label :title %>
      <%= form.text_field :title, class:"form-control" %>
    </div>

    <div class="">
      <%= form.label :body %>
      <%= form.text_area :body, class:"form-control" %>
    </div>

    <div class="actions" style="padding:20px 60px;">
      <%= form.submit "Create Photo", class:"btn btn-success" %>
    </div>
<% end %>

<script type="text/javascript"></script>